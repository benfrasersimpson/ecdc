{
  "containerDefinitions": [
    {
      "name": "foo",
      "image": "foo/bar",
      "cpu": 512,
      "memory": 512,
      "essential": true,
      "environment": [
        { "name": "BUILD_NUMBER", "value": "123" },
        { "name": "SOME_VAR", "value": "BAR" }
      ],
      "portMappings": [
        {
          "containerPort": 9000,
          "hostPort": 9003
        }
      ]
    }
  ],
  "family": "foo"
}
